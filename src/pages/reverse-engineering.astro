---
import Header from "../components/Header.astro";
import Project from "../components/Project.astro";
import Sidebar from "../components/Sidebar.astro";
const projects = [
  {
    title: "Multisim",
    slug: "reversing-multisim",
    desc: [
      "Exploration of reverse-engineering Multisim's offline activation system",
      "Focuses on static analysis methodology, decompiler use, and licensing logic comprehension",
      "Examines how logical patch points influence license verification and software integrity",
    ],
    tech: [
      "C#",
      "dnSpy",
      "de4dot",
      "DIE",
      "Process Dump",
      "Strings",
      "Static Analysis",
    ],
    repo: "https://github.com/JackVega/reversing-multisim",
  },
  {
    title: "Lockdown Browser",
    slug: "reverse-engineering-lockdown-browser",
    desc: [
      "Exploration of Lockdown Browser's internal architecture through runtime interception and API hooking",
      "Demonstrates safe, controlled hook detours using Win32 APIs and Microsoft Detours library",
      "Focuses on defensive engineering principles behind secure input supervision and message interception",
    ],
    tech: [
      "Binary Ninja",
      "Visual Studio",
      "Microsoft Detours",
      "DIE",
      "Win32 API",
      "C++",
      "Hooking",
      "Runtime Interception",
    ],
  },
  {
    title: "Malicious Minecraft Forge Mod",
    slug: "reverse-engineering-minecraft-malware",
    desc: [
      "Static analysis of a Java-based credential stealer disguised as a Minecraft Forge mod",
      "Highlights how threat actors repurpose community distribution formats for malicious payloads",
      "Examines detection heuristics and defensive analysis methods for Java infostealers",
    ],
    tech: ["JD-GUI", "Java", "Static Analysis", "Malware Analysis"],
  },
];
---

<!doctype html>
<html>
  <Header title="Reverse Engineering | Jack Vega" />
  <body>
    <div class="content">
      <Sidebar selectedItem="reverse-engineering" />
      <div class="back-nav-container">
        <a href="/" class="back-nav-button w-inline-block">
          <img
            src="/assets/back.svg"
            loading="lazy"
            alt="Back"
            class="back-nav-img"
          />
          <div>Back</div>
        </a>
      </div>
      <nav class="main">
        <div class="container">
          <div class="content-container">
            <h1>Reversing Projects</h1>
            <section class="items-container">
              <div class="resource-items">
                {
                  projects.map((project) => (
                    <a href={`/projects/${project.slug}`} class="project-card">
                      <Project
                        title={project.title}
                        desc={project.desc}
                        tech={project.tech}
                      />
                    </a>
                  ))
                }
              </div>
            </section>
          </div>
        </div>
      </nav>
    </div>

    <!--[if lte IE 9]><script src="//cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif]-->
  </body><style>
    .main {
      flex: 1;
      display: flex;
      flex-direction: column;
      align-items: center;
      color: #fff;
      overflow-y: auto;
      padding: 2rem 2rem 4rem;
    }

    .main h1 {
      position: relative;
      display: inline-block;
      text-align: left;
      color: #fff;
    }

    .main h1::after {
      content: "";
      position: absolute;
      left: 0;
      bottom: -0.5rem;
      width: 58%;
      height: 3px;
      background: rgba(120, 226, 160, 0.8);
      border-radius: 2px;
    }
  </style>
</html>
